<p id="notice"><%= notice %></p>

<h1>Csv Books</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Id</th>
      <th>Title</th>
      <th>Author</th>
      <th>Date</th>
      <th>Publisher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csv_books.each do |csv_book| %>
      <tr>
        <td><%= csv_book.name %></td>
        <td><%= csv_book.id %></td>
        <td><%= csv_book.title %></td>
        <td><%= csv_book.author %></td>
        <td><%= csv_book.date %></td>
        <td><%= csv_book.publisher %></td>
        <td><%= link_to 'Show', csv_book %></td>
        <td><%= link_to 'Edit', edit_csv_book_path(csv_book) %></td>
        <td><%= link_to 'Destroy', csv_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv Book', new_csv_book_path %>
